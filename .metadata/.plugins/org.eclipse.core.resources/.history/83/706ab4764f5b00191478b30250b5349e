package contro;

import encryption.rSecret;

public class ing {
	public ing(String ida,int type,int num) {
		num=0;
		if(type==1) {
			while(true) {
				if(num<(jsonStr.totals)) {
				num=20+num;
				}else {
					System.out.println("end");
					break;
				}
				new rSecret(ida,type,num);
			}
			
		}else if(type==2) {
			while(true) {
				if(num<(jsonStr.totals)) {
				num=30+num;
				}else {
					System.out.println("end");
					break;
				}
				new rSecret(ida,type,num);
			}
		}else if(type==3) {
			new rSecret(ida,type,num);
		}
		
	}
	public static void main(String[]args) {
		new ing("ÕÅ¹úÈÙ",2,0);
	}
}
